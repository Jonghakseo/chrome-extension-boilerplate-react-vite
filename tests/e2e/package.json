    "@wdio/globals": "^9.1.2",
    "@wdio/local-runner": "^9.1.2",
    "@wdio/mocha-framework": "^9.1.2",
    "@wdio/spec-reporter": "^9.2.8",
    "@wdio/types": "^9.1.2",
    "tsx": "^4.19.1"
  }
}
