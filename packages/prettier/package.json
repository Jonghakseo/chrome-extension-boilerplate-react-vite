{
  "name": "@chrome-extension-boilerplate/prettier-config",
  "version": "0.0.1",
  "description": "prettier config for chrome extension",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write --ignore-path ../../.prettierignore",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@chrome-extension-boilerplate/tsconfig": "workspace:*",
    "eslint": "8.57.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@chrome-extension-boilerplate/eslint-config/base"
    ]
  },
  "prettier": "@chrome-extension-boilerplate/prettier-config"
}
