{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["clean", "^clean", "^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "type-check": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "lint:fix": {
      "cache": false
    },
    "prettier": {
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^test", "^build"
      ],
      "cache": false
    },
    "clean": {
    }
  }
}
